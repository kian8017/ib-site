---
interface Props {
	title: string;
	description: string;
}

let {title, description} = Astro.props as Props;
---

<head>
	<title>{title} | IndexBrain</title>
	<meta charset="UTF-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="description" content={description} />
</head>